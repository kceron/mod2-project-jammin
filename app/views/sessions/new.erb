<h2>Log In</h2>
<% if @errors %>
  <% @errors.each do |error| %>
    <%= error %><br>
  <% end %>
<% end %>

<%= form_tag sessions_path do %>
  <%= label_tag :username %>
  <%= text_field_tag :username %>
  <br><br>
  <%= label_tag :password %>
  <%= password_field_tag :password %>
  <br><br>
  <%= submit_tag "Log In" %>
<% end %>